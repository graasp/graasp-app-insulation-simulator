  ],
  "homepage": ".",
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@graasp/apps-query-client": "3.4.15",
    "@graasp/sdk": "4.9.0",
    "@graasp/ui": "4.17.1",
    "@mui/icons-material": "5.16.7",
    "@mui/lab": "5.0.0-alpha.173",
    "@mui/material": "5.16.7",
    "@react-three/drei": "^9.114.4",
    "@react-three/fiber": "^8.17.10",
    "@sentry/react": "7.119.2",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@types/node": "20.17.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "i18next": "^23.9.0",
    "lucide-react": "^0.456.0",
    "papaparse": "^5.4.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "14.1.3",
    "react-toastify": "10.0.6",
    "three": "^0.169.0",
    "typescript": "5.6.3"
  },
  "scripts": {
    "dev": "yarn vite",
    "start": "yarn dev",
    "start:test": "yarn vite --mode test",
    "build": "yarn vite build",
    "build:test": "yarn vite build --mode test",
    "preview": "yarn vite preview",
    "preview:test": "yarn vite preview --mode test",
    "postinstall": "husky install",
    "lint": "eslint .",
    "prettier:write": "prettier src/**/*.{ts,tsx,js,jsx} --write",
    "prettier:check": "prettier src/**/*.{ts,tsx,js,jsx} --check",
    "type-check": "tsc --noEmit",
    "check": "yarn lint && yarn prettier:check && yarn type-check",
    "hooks:install": "husky install",
    "hooks:uninstall": "husky uninstall",
    "pre-commit": "yarn prettier:check && yarn lint",
    "playwright": "playwright test --project chromium",
    "playwright:ui": "playwright test --ui",
    "playwright:trace": "playwright show-trace",
    "unittest": "vitest",
    "cov:report": "open ./coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.49.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/i18n": "0.13.12",
    "@types/papaparse": "^5.3.15",
    "@types/three": "^0",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "1.7.9",
    "concurrently": "8.2.2",
    "dotenv": "^16.4.5",
    "env-cmd": "10.1.0",
    "eslint": "9.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "globals": "^15.11.0",
    "husky": "9.1.7",
    "nock": "^13.5.3",
    "nyc": "17.1.0",
    "playwright-test-coverage": "^1.2.12",
    "prettier": "3.3.3",
    "three-stdlib": "2.33.0",
    "uuid": "9.0.1",
    "vite": "^5.1.3",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-istanbul": "^6.0.0",
    "vitest": "^2.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/**/*.d.ts"
    ]
  },
  "packageManager": "yarn@4.5.3",
  "msw": {
    "workerDirectory": "public"
  }
}
